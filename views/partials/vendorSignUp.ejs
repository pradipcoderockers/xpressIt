<div class="wrapper body-bg-white">
    <div class="text-center loginscreen animated fadeInDown">
       <div>
          <div class="row">
             <div class="col-md-12 text-center">
                <img style="width:120px;" src="img/fxt_logo.png" class="logo">
             </div>
          </div>
          <div class="row">
             <div class="col-md-12">
                <h2 class="bold">Register your Account here</h2>
             </div>
          </div>
          <form name="addServiceProviderForm" class="m-t" role="form" novalidate ng-submit="!addServiceProviderForm.$valid||vendorCtrl.submit()">
             <div class="row">
                <div class="col-md-4 col-lg-4 text-center">
                  
                    <div class="contact-box" class="no-border">
                   <div class="form-group">
                     
                      <label class="bold"> First Name
                        </label>
                      <input type="text" class="form-control" placeholder="First Name" ng-required="true" name="firstName" ng-model="vendorCtrl.user.firstName" name="firstName">
                   </div>
                   <div ng-if="addServiceProviderForm.firstName.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter first name.</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Last Name
                        </label>
                      <input type="text" class="form-control" placeholder="Last Name" ng-required="true" name="lastName" ng-model="vendorCtrl.user.lastName">
                   </div>
                   <div ng-if="addServiceProviderForm.lastName.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter last name.</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Organization Name
                        </label>
                      <input type="text" class="form-control" placeholder="Organization Name" name="organisation" ng-model="vendorCtrl.user.organisation" ng-required="true">
                   </div>
                   <div ng-if="addServiceProviderForm.organisation.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter organization name.</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Phone Number
                        </label>
                      <input type="text" numbers-only class="form-control" placeholder="Phone Number" ng-required="true" name="phoneNumber" ng-model="vendorCtrl.user.phone" limit-to="10" ng-minlength="10" ng-maxlength="10">
                   </div>
                   <div ng-if="addServiceProviderForm.phoneNumber.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter phone number.</div>
                   </div>
                   <div ng-if="addServiceProviderForm.phoneNumber.$error.minlength">
                      <div class="errors">10 digits only</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Email
                        </label>
                      <input type="email" class="form-control" placeholder="Email" ng-required="true" name="email" ng-model="vendorCtrl.user.email">
                   </div>
                   <div ng-if="addServiceProviderForm.email.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter email</div>
                   </div>
                   <div ng-if="!addServiceProviderForm.email.$error.required && addServiceProviderForm.email.$invalid && vendorCtrl.submitted">
                      <div class="errors">Please enter a valid email</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Password
                        </label>
                      <input type="password" class="form-control" placeholder="Password" ng-required="true" name="password" ng-model="vendorCtrl.user.password">
                   </div>
                   <div ng-if="addServiceProviderForm.password.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter password</div>
                   </div>


                  <select ng-model="vendorCtrl.user.package" ng-init="vendorCtrl.user.package=vendorCtrl.packageList[0]" class="form-control maincats"  name="end" data-id="56">
                    <option ng-repeat="option in vendorCtrl.packageList" ng-value="option">{{option.name}}</option>                 
                  </select>
                    <div class="contact-box" class="no-border">
                       <label class="bold"> Plan  Details
                       </label>
 
                       
                 <table>
                     <tr>
                       <th> Name
                         </th>
                         <th> Description
                           </th>
                           <th> Price
                             </th>
 
                       </tr>
                       <tr >
                         <td>  {{vendorCtrl.user.package.name}}
                         </td>
                         <td>  {{vendorCtrl.user.package.description}}
                        </td>
                        <td>  {{vendorCtrl.user.package.price}}
                        </td>
                        </tr>
                        
                     </table>
 
                    </div>
                 </div>
                  </div>
                  <div class=" col-md-4 col-lg-4 text-center">
                     <div class="contact-box" class="no-border">

                   <div class="form-group">
                     
                      <label class="bold"> City
                        </label>
                      <input type="text" class="form-control" placeholder="city" ng-required="true" name="city" ng-model="vendorCtrl.user.city">
                   </div>
                   <div ng-if="addServiceProviderForm.city.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter city</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Street
                        </label>
                      <input type="text" class="form-control" placeholder="street" ng-required="true" name="street" ng-model="vendorCtrl.user.street">
                   </div>
                   <div ng-if="addServiceProviderForm.street.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter street</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> State
                        </label>
                      <input type="text" class="form-control" placeholder="state" ng-required="true" name="state" ng-model="vendorCtrl.user.state">
                   </div>
                   <div ng-if="addServiceProviderForm.state.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter state</div>
                   </div>
                   <div class="form-group">
                     
                      <label class="bold"> Zip Code
                        </label>
                      <input type="text" numbers-only class="form-control" placeholder="zipcode" ng-required="true" name="zipcode" ng-model="vendorCtrl.user.zipcode" limit-to="6" ng-minlength="5" ng-maxlength="6" ng-focus="vendorCtrl.focus=true;vendorCtrl.blur=false" ng-blur="vendorCtrl.focus=false;vendorCtrl.blur=true">
                   </div>
                   <div ng-if="addServiceProviderForm.zipcode.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter zipcode</div>
                   </div>
                   <div ng-if="addServiceProviderForm.zipcode.$error.minlength && vendorCtrl.blur && !vendorCtrl.focus">
                      <div class="errors">Please aleast 5 digit zip code</div>
                   </div>
                   <div class="form-group" ng-if="vendorCtrl.show">
                     
                      <label class="bold"> Longitude
                        </label>
                      <input type="text" class="form-control" placeholder="longitude" ng-required="true" name="logitude" ng-model="vendorCtrl.user.latLong.long" valid-number allow-decimal="true" allow-negative="true" decimal-upto="10">
                   </div>
                   <div ng-if="addServiceProviderForm.logitude.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter longitude</div>
                   </div>
                   <div class="form-group" ng-if="vendorCtrl.show">
                     
                      <label class="bold"> Latitude
                        </label>
                      <input type="text" class="form-control" placeholder="latitude" ng-required="true" name="latitude" ng-model="vendorCtrl.user.latLong.lat" valid-number allow-decimal="true" allow-negative="true" decimal-upto="10">
                   </div>
                   <div ng-if="addServiceProviderForm.latitude.$error.required && vendorCtrl.submitted">
                      <div class="errors">Please enter latitude</div>
                   </div>
                  <button class="btn btn-primary form-group" ng-click="vendorCtrl.getLocation()">
                      Get My Location
                    </button>
                   
                   <div class="form-group">
                     
                      <label class="bold"> Return Policy
                        </label>
                    <textarea class="form-control" placeholder="Return Policy" name="policy" ng-model="vendorCtrl.user.policy" required="true"></textarea>
                 </div>
                 <div ng-if="addServiceProviderForm.policy.$error.required && vendorCtrl.submitted">
                    <div class="errors">Please enter policy</div>
                 </div>
                 <div class="form-group">
                    <div class="row border-bottom-gray margin-left-10 margin-right-10 margin-bottom-10">
                       <div class="col-sm-12 text-left">
                          <h3>Select Logo</h3>
                       </div>
                    </div>
                    <div ng-if="vendorCtrl.user.file">
                       <img ng-src="{{vendorCtrl.user.file.$ngfBlobUrl}}" class="profileImageBlock">
                    </div>
                    <div class="text-center text-danger" ng-if="addServiceProviderForm.file.$error.required && vendorCtrl.submitted">
                       Please select image
                    </div>
                    <button class="btn btn-primary form-group margin-top-20" ngf-select ng-model="vendorCtrl.user.file" name="file" ngf-pattern="'image/*'" ngf-accept="'image/*'" ngf-max-size="20MB" ngf-min-height="100" required="true">
                    Select
                    </button>
                 </div>
                   </div>
                   </div>
                   
                <div class=" col-md-4 col-lg-4 text-center">
                   <div class="contact-box" class="no-border">
                      <label class="bold"> Select Timing
                      </label>

                      
                <table>
                    <tr>
                      <th> Day
                        </th>
                        <th> Action
                          </th>
                          <th> Start Time
                            </th>
                            <th> End Time
                              </th>

                      </tr>
                      <tr ng-repeat="time in vendorCtrl.user.timing">
                        <td>  {{vendorCtrl.days[$index]}}
                        </td>
                        <td>
                            <input type="checkbox" ng-model="time.value" name="day" >
                        </td>
                        <td><select ng-model="time.startTime" ng-init="time.startTime=vendorCtrl.slots[0]" class="form-control maincats" name="start" data-id="56" required="time.value">
                            <option ng-repeat="slot in vendorCtrl.slots">{{slot}}</option>
                         </select></td>
                         <td>
                            <select ng-model="time.endTime" ng-init="time.endTime=vendorCtrl.slots[0]" class="form-control maincats"  name="end" data-id="56" required="time.value">
                               <option ng-repeat="slot in vendorCtrl.slots">{{slot}}</option>
                            </select></td>
                          </tr>
                    </table>


